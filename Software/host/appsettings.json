{

  "$schema": "http://cachemanager.net/schemas/cachemanager.json#",

  "cacheManagers": [
    {
      "name": "secretCache",
      "handles": [
        {
          "knownType": "MsMemory",
          "expirationMode": "Sliding",
          "expirationTimeout": "0:05"
        }
      ]
    }
  ],

  "ConnectionStrings": {
    "DefaultConnection": "User ID=postgres;Password=postgres;Host=evtbus.aquilon.ru;Port=5433;Database=conf_hall;Pooling=false;"
  },

  "Data": {
    "DefaultConnection": {
      "ProviderName": "Npgsql"
    }
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "TokenAuthentication": {
    "Audience": "Audience",
    "Expiration": "0:05",
    "Issuer": "Astech",
    "SecretPhrase": "secretphrase_secretphrase123!",
    "TwoFactorAuth": true,
    "TwoFactorAuthOptions": { "Padding": "Zeros" }
  }

}
